{
  "name": "artful/polymer",
  "description": "Polymer是一个灵活并且可自由组合的PHP框架,基于slim+doctrine+twig",
  "license": "proprietary",
  "keywords": [
    "Polymer",
    "Slim-MVC",
    "Doctrine",
    "Twig"
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "slim/psr7": "1.4",
    "slim/slim": ">=4",
    "slim/flash": ">=0.4",
    "slim/csrf": ">=1",
    "slim/twig-view": ">=3",
    "tuupola/cors-middleware": ">=1",
    "tuupola/slim-jwt-auth": ">=3",
    "doctrine/inflector": ">=1",
    "doctrine/orm": ">=2",
    "beberlei/doctrineextensions": ">=1",
    "gedmo/doctrine-extensions": ">=3",
    "doctrine/migrations": ">=3",
    "doctrine/annotations": ">=1",
    "monolog/monolog": ">=2",
    "hassankhan/config": ">=2",
    "peppeocchi/php-cron-scheduler": ">=4",
    "symfony/validator": ">=5",
    "symfony/serializer": ">=5",
    "symfony/property-access": ">=5",
    "symfony/property-info": ">=5",
    "symfony/expression-language": ">=5",
    "php-amqplib/php-amqplib": ">=3",
    "php-di/php-di": ">=6",
    "php-di/slim-bridge": ">=3",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*",
    "goaop/framework": ">=3.0.0",
    "cerbero/dto": ">=2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "guzzlehttp/guzzle": ">=6",
    "vlucas/phpdotenv": ">=2",
    "mockery/mockery": ">=0.9",
    "phpunit/phpunit": ">=8",
    "facebook/webdriver": ">=1",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": ">=3",
    "mysql-workbench-schema-exporter/doctrine2-exporter": ">=3"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "src\\",
      "Polymer\\Tests\\": "tests\\"
    },
    "files": [
      "src\\Lib\\functions_include.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
