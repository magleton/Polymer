{
  "name": "artful/polymer",
  "description": "Polymer是一个灵活并且可自由组合的PHP框架,基于slim+doctrine+twig",
  "license": "proprietary",
  "keywords": [
    "Polymer",
    "Slim-MVC",
    "Doctrine",
    "Twig"
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "slim/slim": "3.*",
    "slim/flash": ">=0.2",
    "slim/csrf": ">=0.8",
    "slim/twig-view": ">=2.2",
    "tuupola/cors-middleware": ">=0.5",
    "tuupola/slim-jwt-auth": ">=2.3",
    "doctrine/inflector": ">=1.1",
    "doctrine/orm": ">=2.5",
    "beberlei/DoctrineExtensions": ">=1.0",
    "gedmo/doctrine-extensions": ">=2.4",
    "doctrine/migrations": ">=1.5",
    "monolog/monolog": ">=1.18",
    "hassankhan/config": ">=0.10",
    "peppeocchi/php-cron-scheduler": ">=1.4",
    "symfony/validator": ">=3.0",
    "symfony/serializer": ">=3.0",
    "symfony/property-access": ">=3.0",
    "symfony/property-info": ">=3.1",
    "symfony/expression-language": ">=3.2",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*",
    "goaop/framework": ">=2.3",
    "bernard/bernard": "dev-master",
    "dto/dto": ">=3.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "guzzlehttp/guzzle": "^6.2",
    "vlucas/phpdotenv": "^2.4",
    "runcmf/runtracy": ">=0.2",
    "mikey179/vfsStream": "^1.6",
    "mockery/mockery": "^0.9",
    "satooshi/php-coveralls": "^1.0",
    "phpunit/phpunit": "^5.7",
    "facebook/webdriver": "^1.0",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": ">=3.0",
    "mysql-workbench-schema-exporter/doctrine2-exporter": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "src\\",
      "Polymer\\Tests\\": "tests\\"
    },
    "files": [
      "src\\Lib\\functions_include.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
